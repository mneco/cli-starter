{
    "name": "cli-starter",
    "version": "0.1.0",
    "description": "CLI starter template",
    "main": "dist/server.js",
    "repository": "https://github.com/mneco/cli-starter",
    "author": "ConstNW <constnw@gmail.com>",
    "license": "MIT",
    "scripts": {
        "build": "tsc --skipLibCheck",
        "start": "node ./dist/server.js",
        "distribute": "yarn build && yarn start",
        "develop": "tsc-watch --skipLibCheck --onSuccess \"yarn start\"",
        "test": "yarn jest --forceExit --runInBand",
        "pretty": "prettier --check ./src",
        "lint": "yarn pretty && eslint --ext ts,tsx,json ./src",
        "lint:fix": "eslint --fix ./src/**/*.{ts,tsx}"
    },
    "dependencies": {
        "@hapi/boom": "^10.0.0",
        "axios": "^0.27.2",
        "dotenv": "^16.0.1",
        "envalid": "^7.3.1",
        "lodash": "^4.17.21",
        "mongoose": "^6.4.6"
    },
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^3.3.0",
        "@typegoose/typegoose": "^9.10.1",
        "@types/jest": "^28.1.6",
        "@types/lodash": "^4.14.182",
        "@types/node": "^18.0.6",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.30.7",
        "@typescript-eslint/parser": "^5.30.7",
        "axios-mock-adapter": "^1.21.1",
        "eslint": "^8.20.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest": "^26.6.0",
        "eslint-plugin-no-relative-import-paths": "^1.3.4",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "eslint-plugin-use-decorator": "^0.2.0",
        "http-graceful-shutdown": "^3.1.7",
        "jest": "^28.1.3",
        "module-alias": "^2.2.2",
        "mongodb-memory-server": "^8.7.2",
        "prettier": "^2.7.1",
        "prettier-plugin-pkg": "^0.16.0",
        "source-map-support": "^0.5.21",
        "supertest": "^6.2.4",
        "ts-jest": "^28.0.7",
        "tsc-watch": "^5.0.3",
        "typescript": "^4.7.4"
    },
    "_moduleAliases": {
        "@": "dist"
    },
    "packageManager": "yarn@3.1.0"
}